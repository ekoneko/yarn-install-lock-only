    "tslib": "^1.10.0",
    "whatwg-fetch": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check-git && ./scripts/check-nested-dependencies.sh",
      "pre-push": "npm run check-build-version"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "resolutions": {
    "@shimo/modoc": "^1.1.0",
    "@shimo/runtime-env": "^1.1.2"
  }
}
