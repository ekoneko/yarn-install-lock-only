{
  "name": "@shimo/lizard-service-doc",
  "version": "1.0.0",
  "description": "",
  "author": "yangjunbao <yangjunbao@shimo.im>",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "scripts": {
    "preinstall": "node ./scripts/check-install.js",
    "build": "npm run clean && NODE_ENV=${NODE_ENV:-production} NODE_BUILD_TARGET=${NODE_BUILD_TARGET:-release} webpack-cli --env.cli",
    "build:lizard-local-view": "NODE_DEPLOY_TARGET=lizard-local-view npm run build",
    "clean": "rm -rf cjs esm build temp .cache",
    "start": "NODE_BUILD_CACHE=off npm run start:fast",
    "start:clean": "npm run clean && npm run start:fast",
    "start:fast": "NODE_ENV=${NODE_ENV:-development} NODE_BUILD_TARGET=${NODE_BUILD_TARGET:-develop} webpack-dev-server --progress --color --env.devServer",
    "analyze": "NODE_BUNDLE_ANALYZE=1 TEMP_OSS_ENABLE= TEMP_SENTRY_ENABLE= TEMP_LIZARD_VIEW_INFO_FILE= npm run build",
    "test": "echo Done",
    "check-build-version": "./check-build-version.sh",
    "check-git": "./check-git.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@git.shimo.im:shimo/lizard-service-doc.git"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@shimo/web-starter-build": "^7.22.7",
    "@shimo/web-starter-runtime": "^7.12.1",
    "@types/node": "^8.10.55",
    "babel-loader": "^7.1.5",
    "event-stream": "3.3.4",
    "hoist-non-react-statics": "^3.3.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "typescript": "^3.6.4",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@shimo/ad-platform-sdk": "^1.0.27",
    "@shimo/collaborators-client": "^1.2.2",
    "@shimo/common": "^2.6.16",
    "@shimo/common-js": "^1.0.53",
    "@shimo/intercom": "^1.2.7",
    "@shimo/lc-download": "^1.2.4",
    "@shimo/lc-feedback": "^1.1.6",
    "@shimo/lc-file-create": "^1.0.8",
